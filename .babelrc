{
	"presets": [
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": "3",
				"modules": false,
				"debug": false
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
		"@babel/plugin-proposal-export-default-from",
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		]
	],
	"env": {
		"test": {
			"presets": [
				[
					"@babel/preset-env",
					{
						"useBuiltIns": "usage",
						"corejs": "3",
						"debug": false
					}
				]
			]
		}
	}
}
